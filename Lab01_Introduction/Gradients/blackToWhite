<body>
<script src="//cdnjs.cloudflare.com/ajax/libs/p5.js/0.5.7/p5.js"></script>
<script>
function setup() {
	createCanvas(800,600);
  background(0);
}
function draw() {  
	//noprotect
	for(y=0; y<height; y++) {
  	for(x=0; x<width; x++) {
    		set(x, y, color(256*(x/width)));
    }
  }
  updatePixels();
}
</script>
</body>

